<html><head><meta http-equiv="Content-Type" content="text/html; charset=ISO-8859-1"><style type="text/css"></style><style type="text/css"></style></head><body><pre style="word-wrap: break-word; white-space: pre-wrap;">#!/system/bin/sh

if [[ -f /system/bin/mpdecision ]];
then
     mount -o remount,rw /system
     mv /system/bin/mpdecision /system/bin/mpdecisionRENAMED
     mount -o remount,ro /system
else
     busybox echo "File already renamed or it doesn't exist"
fi
